{
  "name": "vscode-tabset",
  "displayName": "vscode-tabset",
  "description": "Manages groups of tabs",
  "version": "0.0.1",
  "publisher": "jacobdufault",
  "engines": {
    "vscode": "^1.23.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "tabset.switch",
        "category": "tabset",
        "title": "Switch"
      },
      {
        "command": "tabset.new",
        "category": "tabset",
        "title": "New"
      },
      {
        "command": "tabset.delete",
        "category": "tabset",
        "title": "Delete"
      },
      {
        "command": "tabset.rename",
        "category": "tabset",
        "title": "Rename"
      },
      {
        "command": "tabset.reset",
        "category": "tabset",
        "title": "Reset all state"
      },
      {
        "command": "tabset.iconClick",
        "category": "tabset",
        "title": "Dispatch to other tabset commands"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "tabset.iconClick",
          "when": "false"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "@types/node": "^7.0.43"
  }
}
